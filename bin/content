                      dclick = $alink.attr("data-click");
                      dhref = $alink.attr("href");
                      ol = alink.offsetLeft;
                      ot = alink.offsetTop;
                      w = $alink.width();
                      h = $alink.height();
                      rw = $alink.find("img").width();
                      rh = $alink.find("img").height();
                      scale = 0.1;
                      nw = w + w*scale*2;
                      nh = h + h*scale*2;
                      //放大的高度不宜过高
                      if(scale*h > 10){
                          scale = 10/h;
                          nh = h + h*0.2;
                          nw = w + w*scale*2;
                      }
                      $img.stop().animate({
                          width : rw + rw*scale*2 + "px",
                          height : rh + rh*scale*2 + "px"
                      },100);
