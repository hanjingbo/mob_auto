                        $(dom).attr('data-isloaded', false);
                        $(dom).attr('data-timgUrl', '');
                      } else {
                        $(dom).attr('data-isloaded', true);
                        $(dom).attr('data-timgUrl', timgUrl);
                      }
                    }).bind('error', function () {
                      $(dom).attr('data-isloaded', false);
                      $(dom).attr('data-timgUrl', '');
                    });
                    img.src = decodeURIComponent(timgUrl);
                }
              });
